{
	"name": "emirdeliz-dart-multiple-repository-utils",
	"displayName": "Dart utils to multi-repo approach",
	"description": "emirdeliz-dart-multiple-repository-utils - Makes your life easier to use the dart if you work with a multi-repo approach",
	"publisher": "emirdeliz",
	"homepage": "https://github.com/emirdeliz/emirdeliz-dart-multiple-repository-utils",
	"bugs": {
		"url": "https://github.com/emirdeliz/emirdeliz-dart-multiple-repository-utils/issues",
		"email": "emirdeliz@gmail.com"
	},
	"license": "MIT",
	"main": "./dist/extension.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/emirdeliz/emirdeliz-dart-multiple-repository-utils.git"
	},
	"categories": [
		"Programming Languages",
		"Other"
	],
	"keywords": [
		"dart",
		"dart utils",
		"dart pub get",
		"dart clean",
		"flutter",
		"flutter utils",
		"flutter pub get",
		"flutter clean"
	],
	"icon": "assets/icon.png",
	"galleryBanner": {
		"color": "#C80000",
		"theme": "dark"
	},
	"version": "0.0.1",
	"type": "commonjs",
	"engines": {
		"vscode": "^1.71.0"
	},
	"activationEvents": [
		"onCommand:emirdeliz-dart-multiple-repository-utils.disposableInitialize",
		"onCommand:emirdeliz-dart-multiple-repository-utils.clean-pub-get",
		"onCommand:emirdeliz-dart-multiple-repository-utils.clean-pub-get-workspace",
		"onCommand:emirdeliz-dart-multiple-repository-utils.pull-workspace"
	],
	"extensionDependencies": [],
	"contributes": {
		"commands": [
			{
				"command": "disposableInitialize",
				"title": "Initialize",
				"description": "Initialize extension"
			},
			{
				"command": "emirdeliz-dart-multiple-repository-utils.clean-pub-get",
				"title": "Clean and Pub Get",
				"description": "Run clean and then pub get for the focus project"
			},
			{
				"command": "emirdeliz-dart-multiple-repository-utils.clean-pub-get-workspace",
				"title": "Clean and Pub Get Workspace",
				"description": "Run clean and then pub get for each project on the root of the workspace."
			},
			{
				"command": "emirdeliz-dart-multiple-repository-utils.pull-workspace",
				"title": "Pull Workspace",
				"description": "Run git pull for each project on the root of the workspace."
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder && resourceFilename == pubspec.yaml",
					"command": "emirdeliz-dart-multiple-repository-utils.clean-pub-get",
					"group": "emirdeliz-dart-multiple-repository-utils@1"
				},
				{
					"when": "explorerResourceIsFolder && resourceFilename == pubspec.yaml",
					"command": "emirdeliz-dart-multiple-repository-utils.clean-pub-get-workspace",
					"group": "emirdeliz-dart-multiple-repository-utils@2"
				},
				{
					"when": "explorerResourceIsRoot && config.git.enabled",
					"command": "emirdeliz-dart-multiple-repository-utils.pull-workspace",
					"group": "emirdeliz-dart-multiple-repository-utils@3"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run build:dist",
		"vscode:package": "yarn run build:dist && vsce package --yarn",
		"vscode:publish": "yarn vscode:package && vsce publish --yarn",
		"build:dist": "swc ./src --quiet --out-dir dist",
		"build:dist:watch": "swc ./src --watch --quiet --out-dir dist",
		"build:test": "swc ./src/test --out-dir dist",
		"test": "jest --clearCache && jest ./src/test/extension.test.ts",
		"test:watch": "jest --watch ./src/test/extension.test.ts",
		"symbol:link:modules:pre": "rm -rf node_modules/emirdeliz-vs-extension-utils",
		"symbol:link:modules": "yarn run symbol:link:modules:pre && ln -s ../../emirdeliz-vs-extension-utils node_modules/emirdeliz-vs-extension-utils",
		"lint:check": "eslint src --ext .ts --fix",
		"clear:dependencies": "rm -rf node_modules && rm -rf yarn.lock",
		"install:dev": "yarn clear:dependencies && yarn install && yarn symbol:link:modules"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.71.0",
		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@typescript-eslint/parser": "^5.31.0",
		"@vscode/test-electron": "^2.1.5",
		"eslint": "^8.20.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"emirdeliz-vs-extension-utils": "../emirdeliz-vs-extension-utils"
	}
}